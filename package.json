    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
